{
    "name": "graphlr/prepr-api-laravel",
    "description": "Prepr API wrapper for Laravel",
    "keywords": ["laravel", "prepr", "preprforradio"],
    "license": "MIT",
    "authors": [
        {
            "name": "Prepr",
            "email": "support@prepr.io"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Graphlr\\Prepr\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Graphlr\\Prepr\\PreprServiceProvider"
            ],
            "aliases": {
                "Prepr": "Graphlr\\Prepr\\PreprFacade"
            }
        }
    }
}
